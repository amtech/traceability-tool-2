      <metadata name="net.sf.eclipsecs.core.lastEnabledSeverity" value="inherit"/>
    </module>
    <module name="HiddenField">
      <property name="tokens" value="PARAMETER_DEF,VARIABLE_DEF,LAMBDA"/>
